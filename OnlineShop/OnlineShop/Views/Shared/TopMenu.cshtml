@model List<Model.EF.Menu>

    <div class="account_desc">

        @{ var session = (OnlineShop.Common.UserLogin)Session[OnlineShop.Common.CommonContaist.USER_SESSION]; }
        @if (session == null)
        {
        <ul>
            @foreach (var item in Model)
            {
        <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
            }
        </ul>

        } else {
          <ul>
              <li>Xin chào: @session.UserName</li>
              <li>@Html.ActionLink("Đăng suất","Loguot","User")</li>
          </ul>
        }
                </div>

